{
  "name": "underwire",
  "version": "0.0.3",
  "description": "Dependency tracking system",
  "main": "./bin/server",
  "scripts": {
    "start": "node ./bin/server",
    "dev": "nodemon ./bin/serverdev --hot",
    "test": "lab --transform ./test/setup.js --ignore 'Reflect,core,_babelPolyfill,regeneratorRuntime,__core-js_shared__'",
    "compile-ui": "NODE_ENV=production rm -rf web/site/; webpack --colors --display-error-details --config configs/webpack.client.js",
    "compile": "npm run compile-ui",
    "preuninstall": "rm -rf dist/; rm -rf web/site/; rm -rf plugins/hapi-swagger/node_modules/",
    "install": "npm run compile"
  },
  "dependencies": {
    "async": "^1.5.2",
    "babel-cli": "^6.6.x",
    "babel-core": "^6.7.x",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.x",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.x",
    "babel-register": "^6.7.x",
    "codemirror": "^5.17.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "h2o2": "^5.1.0",
    "handlebars": "^4.0.5",
    "hapi": "^14.0.0",
    "hoek": "^4.0.2",
    "html-webpack-plugin": "^2.15.0",
    "http-status": "^0.2.3",
    "inert": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^9.0.4",
    "json-loader": "^0.5.4",
    "json-schema-ref-parser": "^3.1.2",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "moment": "^2.14.1",
    "npm": "^3.10.5",
    "radium": "^0.17.0",
    "react": "^0.14.8",
    "react-bootstrap": "^0.28.4",
    "react-codemirror": "^0.2.6",
    "react-dom": "^0.14.8",
    "react-intl": "^2.0.0-rc-1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-select": "^1.0.0-beta14",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "sift": "^3.2.4",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "uuid": "^2.0.2",
    "vision": "^4.0.1",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "code": "^2.1.0",
    "lab": "^10.3.0",
    "nodemon": "^1.8.1",
    "react-transform-hmr": "^1.0.4",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "author": "Jeremy Darling <jeremy.darling@gmail.com>",
